                    },
                    {
                        "table": "SourceSystem",
                        "table_primary_key": "_c0"
                    }
                ]
            }
        },
        "variables": {
            "defaultFilename": {
                "type": "String",
                "defaultValue": "test_data.csv"
            }
        },
        "folder": {
            "name": "Insights Module/basic"
        },
        "annotations": [],
        "lastPublishTime": "2022-12-27T18:53:00Z"
    },
    "type": "Microsoft.Synapse/workspaces/pipelines"
